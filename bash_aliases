##.bash_aliases

alias cl='clear'
alias lf='~/gitrepos/scripts/list-folder.sh'
alias l='ls -l'
alias la='ls -a'
alias bu='~/gitrepos/scripts/backup.sh'

## git shortcuts
alias gb='git branch'
alias ga='git add'
alias gch='git checkout'
alias gs='git status'
alias gco='git commit -m'
alias gl='git log'
